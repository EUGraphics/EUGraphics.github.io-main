  font-family: montserrat;
  font-weight: 500;
}

#logo-graphics {
  font-family: montserrat;
  font-weight: 200;
}


#menu-btn:focus, #hm-btn:focus{
  outline:0;
}

.nooverflow {
  overflow: hidden;
}
.hamburger {
  opacity:1;
}


.hamburger-inner, .hamburger-inner::before, .hamburger-inner::after {
  width: 24px;
  height: 1px;
  background-color: rgba(255,255,255,1);
}


.header-menu  {
  position: relative;
  z-index: 100;
  text-align: center;
  padding: 0;
  margin: 0;
  float: right;
  height: 100vh;
  width: 100vw;
  right: -100vw;
  background-color: #000;
  opacity: 0.6;
  vertical-align: bottom;
}

.header-menu.is-active {
  -webkit-animation: moveIn .5s ease-in-out both;
  animation: moveIn .5s ease-in-out both;
}

.header-menu.inactive {
  -webkit-animation: moveOut .5s ease-in-out both;
  animation: moveOut .5s ease-in-out both;
}

@keyframes moveIn {
    0% {
        right: -100vw;
  }
    100% {
        right: 0vw;
    }
}


@keyframes moveOut {
    0% {
        right: 0vw;
  }
    100% {
        right: -100vw;
    }
}


.landing-pg {
  top: 0;
  left: 0;
  position: absolute;
  z-index: 1;
  object-fit: none;
  height: 100%;
  width: 100%;
  background-image: url('/assets/img/landing-bg.jpg');
  background-repeat: no-repeat;
  background-size: cover;
}

@keyframes fadein {
  0% { opacity: 0; }
    50%  { opacity: 0.45; }
    100% { opacity: 0.3;}
}

/* Firefox < 16 */
@-moz-keyframes fadein {
  0% { opacity: 0; }
    50%  { opacity: 0.45; }
    100% { opacity: 0.3;}
}

/* Safari, Chrome and Opera > 12.1 */
@-webkit-keyframes fadein {
  0% { opacity: 0; }
    50%  { opacity: 0.45; }
    100% { opacity: 0.3;}
}

/* Internet Explorer */
@-ms-keyframes fadein {
  0% { opacity: 0; }
    50%  { opacity: 0.45; }
    100% { opacity: 0.3;}
}

/* Opera < 12.1 */
@-o-keyframes fadein {
  0% { opacity: 0; }
    50%  { opacity: 0.45; }
    100% { opacity: 0.3;}
}



#landing-gradients {
  pointer-events: none;
  content: " ";
  height: 100%;
  width: 100%;
  position: absolute;
  overflow: hidden;
  z-index: 20;
  top: 0;
  opacity: 0.3;
  left: 0;
  background: -webkit-linear-gradient(rgba(0,0,0,0) 80%, rgba(57,128,190,90) 100%),
              -webkit-radial-gradient(top center, ellipse cover, rgba(255,255,255,0.3) 0%, rgba(0,0,0,0) 60%);
  -webkit-animation: fadein 2s; /* Safari, Chrome and Opera > 12.1 */
  -moz-animation: fadein 2s; /* Firefox < 16 */
  -ms-animation: fadein 2s; /* Internet Explorer */
  -o-animation: fadein 2s; /* Opera < 12.1 */
  animation: fadein 2s;

  -webkit-transform: translate3d(0,0,0);
}

