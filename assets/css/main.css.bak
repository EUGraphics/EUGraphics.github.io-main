.quotepic-container {
  transform: translateY(50%);
  position: relative;
  float: right;
  text-align: center;
}

.quote-text {
  font-family: lato;
  font-weight: bolder;
  font-size: 140% ;
  white-space: pre-line;
  text-overflow:ellipsis;
  color: #fff;
  position:absolute;
  text-align:left;
  width: 90%;
  padding-left: 10%;
}

blockquote {
quotes: "\201C""\201D""\2018""\2019";
}

blockquote h3 {
  font-family: lato;
  font-weight: bolder;
  font-size: 32px;

}

blockquote h3:before {
    position: absolute;
    content: open-quote;
    font-family:  Georgia, "Times New Roman", Times, serif;
    font-size: 72px;
    font-weight: bold;
    overflow: visible;
    left: 10%;
}
blockquote h3:after {
    z-index: 20;
    content: close-quote;
    font-family:  Georgia, "Times New Roman", Times, serif;
    font-size: 72px;
    font-weight: bold;
    float: right;
    padding-right: 60px;
}


.profiles {
  transform: translateY(50%);
  z-index: 100;
}

.rm-btn {
  display: inline-block;
  -webkit-backface-visibility: hidden;
  cursor: pointer;
  transition: 0.4s;
  opacity: 1;
}

.rm-btn:after {
  content: '';
  display: block;
  border-bottom: solid 2px #fff;
  transform: scaleX(0);
  transition: transform 0.4s ease-in-out;
  opacity: 0.65;
}

.rm-btn:hover {
  opacity: 0.65;
}

.rm-btn:hover:after {
  transform: scaleX(1);
}


